{
  "name": "rte-api",
  "version": "0.1.0",
  "description": "Real Time Events API",
  "main": "src/server.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn  --transpile-only -- ./src/server.ts ",
    "dev:debug": "node --inspect --require ts-node/register/transpile-only -- ./src/server.ts",
    "prod": "tsc && node ./build/app.js",
    "sequelize": "sequelize-cli",
    "sequelize-cli": "sequelize-cli",
    "db:create": "sequelize-cli db:create",
    "db:drop": "sequelize-cli db:drop",
    "db:init": "npm run db:create; npm run db:migrate; npm run db:seed",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:g": "sequelize-cli seed:generate --name",
    "model:generate": "sequelize-cli model:generate",
    "env:init": "for file in .env.ul.local .env.ul .env.dl.local .env.dl; do cp example.env \"$file\"; done",
    "env:dl": "rm .env; cp .env.dl .env",
    "env:dl:local": "rm .env; cp .env.dl.local .env",
    "env:ul": "rm .env; cp .env.ul .env",
    "env:ul:local": "rm .env; cp .env.ul.local .env"
  },
  "author": "Brandon Drake",
  "license": "ISC",
  "dependencies": {
    "argon2": "^0.28.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "event-dispatch": "^0.4.1",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "fast-rbac": "^2.0.0",
    "handy-redis": "^2.3.1",
    "inflection": "^1.13.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "node-abort-controller": "^3.0.0",
    "node-cron": "^3.0.0",
    "node-fetch": "^2.6.1",
    "pako": "^2.0.3",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.5",
    "socket.io": "^2.3.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.7",
    "@babel/register": "^7.14.5",
    "@types/crypto-js": "^4.0.2",
    "@types/express": "^4.17.7",
    "@types/express-jwt": "^6.0.2",
    "@types/inflection": "^1.5.28",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node": "^16.3.0",
    "@types/node-cron": "^2.0.4",
    "@types/node-fetch": "^2.5.7",
    "@types/pako": "^1.0.2",
    "@types/redis": "^2.8.31",
    "@types/socket.io": "^2.1.11",
    "@types/uuid": "^8.3.1",
    "@types/validator": "^13.6.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "dotenv": "^8.2.0",
    "node-pg-migrate": "^5.10.0",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^2.1.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.1.8",
    "typedi": "^0.8.0",
    "typescript": "^3.9.7"
  }
}
